{
  "expo": {
    "name": "nativeApp",
    "slug": "nativeApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./app/assets/image/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./app/assets/image/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./app/assets/image/adaptive-icon.png",
        "backgroundColor": "#48cae4"
      },
      "package": "m.ashrafi.nativeApp"
    },
    "web": {
      "favicon": "./app/assets/image/favicon.png"
    },
    "plugins": [
            "./app/native-plugins/injectedAndroidConfig.js",
     [
        "expo-build-properties",
        {
          "android": {
            "enableProguardInReleaseBuilds": true,
            "enableSeparateBuildPerCPUArchitecture": true,
            "usesCleartextTraffic": true
          }
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "2b916102-7832-47ee-a5d6-b119a9ec539f"
      }
    },
    "owner": "m.ashrafi",
    "jsEngine": "hermes"
  }
}
